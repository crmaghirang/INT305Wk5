    var body: some View {
    TabView {
            Text("Search")
                .tabItem {
                    Image(systemName: "magnifyingglass")
                    Text("Search")
            }
            Text("User Profile")
                .tabItem {
                    Image(systemName: "person.circle.fill")
                    Text("Me")
            }
            Text("Saved Searches")
                .tabItem {
                    Image(systemName: "heart.fill")
                    Text("Saved")
            }
            Text("More")
                .tabItem {
                    Image(systemName: "ellipsis")
                    Text("More")
            }
        }
}

